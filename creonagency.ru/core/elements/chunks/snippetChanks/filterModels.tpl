<div class="filter_parent" id="pdopage">
    [[!mFilter2?
        &parents=`[[*id:is=`6`:then=`-198,-389,28,29,30`:else=`[[*id]]`]]`
        &element=`ms2GalleryResources`
        &paginator=`pdoPage`
        &ajaxMode=`button`
        &ajaxTplMore=`@INLINE <div class="recall btn-more"><a>Загрузить еще</a></div>`
        &limit=`21`
        &filters=`
            [[*id:is=`389`:then=``:else=`tv|height`]],
            [[*id:is=`389`:then=``:else=`tv|hair_color_filtr`]],
            [[*id:is=`389`:then=`tv|sex,tv|date_of_birth`:else=`tv|english_filtr`]],`
        &tplOuter=`tpl.mFilter2.outer.custom`
        &tpl=`model_tpl`
        &includeTVs = `model_photo,model_id`
        &processTVs = `model_photo`
        &tvPrefix=``
        &includeThumbs=`360x270`
        &depth=`2`
        &sortby = `menuindex`
        &sortdir = `ASC`
        &tplFilter.outer.default=`tpl.mFilter2.filter.outer.custom`
        &tplFilter.outer.tv|height=`tpl.mFilter2.filter.select.custom.height`
        &tplFilter.row.tv|height=`tpl.mFilter2.filter.option.custom`
        &tplFilter.outer.tv|hair_color_filtr=`tpl.mFilter2.filter.select.custom.hair_color`
        &tplFilter.row.tv|hair_color_filtr=`tpl.mFilter2.filter.option.custom`
        &tplFilter.outer.tv|english_filtr=`tpl.mFilter2.filter.select.custom.english`
        &tplFilter.row.tv|english_filtr=`tpl.mFilter2.filter.option.custom`
        &tplFilter.outer.tv|sex=`tpl.mFilter2.filter.select.custom.sex`
        &tplFilter.row.tv|sex=`tpl.mFilter2.filter.option.custom`
        &tplFilter.outer.tv|date_of_birth=`tpl.mFilter2.filter.select.custom.date_of_birth`
        &tplFilter.row.tv|date_of_birth=`tpl.mFilter2.filter.option.custom`
    ]]

</div>